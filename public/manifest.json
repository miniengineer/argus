{
  "manifest_version": 2,
  "name": "Track websites",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["tabs", "storage"],
  "browser_action": {
    "default_icon": "data.png",
    "default_popup": "index.html",
    "default_title": "Show statistics"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
